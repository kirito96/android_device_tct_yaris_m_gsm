# mount point	fstype    device    	[device2]

/boot      emmc  /dev/bootimg
/recovery  emmc  /dev/recovery
/system    ext4  /dev/block/mmcblk0p4
/cache     ext4  /dev/block/mmcblk0p5
/data      ext4  /dev/block/mmcblk0p6
